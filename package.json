{
  "name": "kevindhumphries",
  "version": "0.0.0",
  "description": "A personal portfolio site built using concepts of Douglas Minnaar's react movie cards and IBM Carbon Design Language",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdhumphries/kdhumphries.github.io.git"
  },
  "homepage": "http://kdhumphries.github.io/",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@carbon/charts": "0.26.0",
    "@carbon/charts-react": "0.26.0",
    "@carbon/colors": "^10.3.0",
    "@carbon/grid": "^10.3.0",
    "@carbon/icons-react": "^10.6.0",
    "apollo-boost": "^0.4.2",
    "carbon-components": "^10.3.0",
    "carbon-components-react": "^7.9.0",
    "d3": "5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "event-stream": "^4.0.1",
    "graphql": "^14.3.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "gh-pages": "^2.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
