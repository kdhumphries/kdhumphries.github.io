{
  "name": "kevindhumphries",
  "version": "0.0.0",
  "description": "A personal portfolio site built using concepts of Douglas Minnaar's react movie cards and IBM Carbon Design Language",
  "repository": {
    "type": "git",
    "url": "https://github.com/kdhumphries/kdhumphries.github.io"
  },
  "homepage": "http://kdhumphries.github.io/kdh",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@carbon/charts": "0.26.0",
    "@carbon/charts-react": "0.26.0",
    "@carbon/colors": "^10.3.0",
    "@carbon/grid": "^10.3.0",
    "@carbon/icons-react": "^10.6.0",
    "apollo-boost": "^0.4.2",
    "carbon-components": "^10.3.0",
    "carbon-components-react": "^7.9.0",
    "d3": "5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "event-stream": "^4.0.1",
    "gh-pages": "^2.2.0",
    "graphql": "^14.3.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "live-server": "^1.2.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.17.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
